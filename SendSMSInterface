        class SmsServiceInterface {
            if $phoneNumbers [1] = 727 then SmsServiceInterface->SmsService1;
            if $phoneNumbers [1] = 717 then SmsServiceInterface->SmsService2;
            if $phoneNumbers [1] = 812 then SmsServiceInterface->SmsService3;
        }
    }
     }
